{
  "kind": "collectionType",
  "collectionName": "curriculum_faculties",
  "info": {
    "singularName": "curriculum-faculty",
    "pluralName": "curriculum-faculties",
    "displayName": "Curriculum.faculty",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "curriculum_school": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::curriculum-school.curriculum-school",
      "inversedBy": "curriculum_faculties"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "curriculum_majors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::curriculum-major.curriculum-major",
      "mappedBy": "curriculum_faculty"
    },
    "curriculum_subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::curriculum-subject.curriculum-subject",
      "mappedBy": "curriculum_faculties"
    },
    "websiteFacultyUrl": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "addressContactFaculty": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phoneNumberContact": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "mailContact": {
      "type": "email",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "facultyCode": {
      "type": "uid",
      "required": true
    }
  }
}

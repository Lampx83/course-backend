{
  "kind": "collectionType",
  "collectionName": "curriculum_majors",
  "info": {
    "singularName": "curriculum-major",
    "pluralName": "curriculum-majors",
    "displayName": "Curriculum.major",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "majorCode": {
      "type": "uid"
    },
    "admissionCode": {
      "type": "uid"
    },
    "enrollmentTarget": {
      "type": "integer"
    },
    "thumbnails": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "introduction": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "graduateOutcomes": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "careerOpportunities": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "syllabuses": {
      "type": "component",
      "repeatable": true,
      "component": "major.syllabus"
    },
    "curriculum_faculty": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::curriculum-faculty.curriculum-faculty",
      "inversedBy": "curriculum_majors"
    },
    "curriculum_school": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::curriculum-school.curriculum-school",
      "inversedBy": "curriculum_majors"
    },
    "credits": {
      "type": "string"
    },
    "curriculum_curricula": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::curriculum-curriculum.curriculum-curriculum",
      "mappedBy": "curriculum_major"
    }
  }
}

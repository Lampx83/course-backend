{
  "kind": "collectionType",
  "collectionName": "curriculum_schools",
  "info": {
    "singularName": "curriculum-school",
    "pluralName": "curriculum-schools",
    "displayName": "Curriculum.school",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "curriculum_faculties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::curriculum-faculty.curriculum-faculty",
      "mappedBy": "curriculum_school"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "curriculum_majors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::curriculum-major.curriculum-major",
      "mappedBy": "curriculum_school"
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "website": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "schoolCode": {
      "type": "uid",
      "required": true
    }
  }
}
